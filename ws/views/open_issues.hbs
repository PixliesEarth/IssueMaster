
<div id="wrapper">
        <nav class="navbar navbar-dark align-items-start sidebar sidebar-dark accordion bg-gradient-primary p-0">
            <div class="container-fluid d-flex flex-column p-0">
                <hr class="sidebar-divider my-0">
                <ul class="nav navbar-nav text-light" id="accordionSidebar">
                    <li class="nav-item" role="presentation"></li>
                    <li class="nav-item" role="presentation"><a class="nav-link active" href="/"><i class="fas fa-tasks"></i><span>Open Issues</span></a></li>
                    <li class="nav-item" role="presentation"></li>
                    <li class="nav-item" role="presentation"><a class="nav-link" href="/submitissues"><i class="far fa-edit"></i><span>Submit an Issue</span></a></li>
                    <li class="nav-item" role="presentation"></li>
                    <li class="nav-item" role="presentation"></li>
                    <li class="nav-item" role="presentation"></li>
                </ul>
                <div class="text-center d-none d-md-inline"><button class="btn rounded-circle border-0" id="sidebarToggle" type="button"></button></div>
            </div>
        </nav>
        <div class="d-flex flex-column" id="content-wrapper">
            <div id="content">
                <nav class="navbar navbar-light navbar-expand bg-white shadow mb-4 topbar static-top">
                    <div class="container-fluid"><button class="btn btn-link d-md-none rounded-circle mr-3" id="sidebarToggleTop" type="button"><i class="fas fa-bars"></i></button><span>Pixlies Earth Issue Tracker</span>
                        <form class="form-inline d-none d-sm-inline-block mr-auto ml-md-3 my-2 my-md-0 mw-100 navbar-search">
                            <div class="input-group"></div>
                        </form>
                    </div>
                </nav>
                <div class="container-fluid">
                    <div class="row" style="color: rgb(0,30,255);">
                        <div class="col-md-6 col-xl-3 mb-4" style="color: rgb(21,43,207);">
                            <h1 style="width: 221px;color: rgb(69,82,176);font-size: 25px;">Suggestions</h1>
                            {{#each suggestions}}    
                            <div class="jumbotron text-center"><small class="text-center" style="margin: 4px;padding: 3px;font-size: 10px;color: rgb(40,40,41);">ID: {{this.id}}</small>
                                <p> {{this.desc}} </p><i class="fa fa-user-o"></i><small class="text-center" style="color: rgb(40,40,41);">&nbsp;{{this.author}}</small>
                                <p></p>
                            </div>
                            {{/each}}
                        
                        </div>
                        <div class="col-md-6 col-xl-3 mb-4">
                            <h1 style="width: 221px;color: rgb(69,82,176);font-size: 25px;">Open Issues</h1>
                            {{#each openIssues}}
                            <div class="jumbotron text-center"><small class="text-center" style="margin: 4px;padding: 3px;font-size: 10px;color: rgb(40,40,41);">ID: {{this.id}}</small>
                                <p> {{this.desc}} </p><i class="fa fa-user-o"></i><small class="text-center" style="color: rgb(40,40,41);">&nbsp;{{this.author}}</small>
                                <p></p>
                                <div class="col"><i class="fa fa-tag"></i><small class="text-danger" data-toggle="tooltip" data-bs-tooltip="" data-placement="bottom" style="filter: saturate(200%);color: rgb(231,185,67);font-size: 14px;" title="This issue has not been reviewed yet.">&nbsp;open</small></div>
                            </div>
                            {{/each}}
                        </div>
                        <div class="col-md-6 col-xl-3 mb-4">
                            <h1 style="width: 221px;color: rgb(69,82,176);font-size: 25px;">Active Issues</h1>
                            {{#each activeIssues}}
                            <div class="jumbotron text-center"><small class="text-center" style="margin: 4px;padding: 3px;font-size: 10px;color: rgb(40,40,41);">ID: {{this.id}}</small>
                                <p> {{this.desc}} </p><i class="fa fa-user-o"></i><small class="text-center" style="color: rgb(40,40,41);">&nbsp;{{this.author}}</small>
                                <p></p>
                                <div class="col"><i class="fa fa-tag"></i><small class="text-warning" data-toggle="tooltip" data-bs-tooltip="" data-placement="bottom" style="filter: contrast(67%) saturate(156%);color: #fbff00;font-size: 14px;" title="This issue is marked active, and is actively being worked on.">&nbsp;active</small></div>
                            </div>
                            {{/each}}
                        </div>
                        <div class="col-md-6 col-xl-3 mb-4">
                            <h1 style="width: 221px;color: rgb(69,82,176);font-size: 25px;">Closed Issues</h1>
                            {{#each closedIssues}}
                            <div class="jumbotron text-center"><small class="text-center" style="margin: 4px;padding: 3px;font-size: 10px;color: rgb(40,40,41);">ID: {{this.id}}</small>
                                <p> {{this.desc}} </p><i class="fa fa-user-o"></i><small class="text-center" style="color: rgb(40,40,41);">&nbsp;{{this.author}}</small>
                                <p></p>
                                <div class="col"><i class="fa fa-tag"></i><small class="text-success" data-toggle="tooltip" data-bs-tooltip="" data-placement="bottom" style="filter: contrast(72%) saturate(200%);color: #60dd49;font-size: 14px;" title="This issue was successfully closed.">&nbsp;done</small></div>
                            </div>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
            <footer class="bg-white sticky-footer">
                <div class="container my-auto">
                    <div class="text-center my-auto copyright"><span style="margin: 11px;">Made with</span><i class="fa fa-heart" style="margin: 0px;"></i><span style="margin: 11px;">by MeerBiene</span></div>
                </div>
            </footer>
        </div><a class="border rounded d-inline scroll-to-top" href="#"><i class="fas fa-angle-up"></i></a></div>